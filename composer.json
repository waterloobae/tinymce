{
    "name": "waterloobae/tinymce",
    "description": "TinyMCE and LaTeX support for Laravel, Livewire, and Filament",
    "keywords": [
        "laravel",
        "livewire",
        "filament",
        "tinymce",
        "latex",
        "mathjax",
        "rich-editor"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Waterloo Bae",
            "email": "terry.bae@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "filament/filament": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Waterloobae\\TinyMce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waterloobae\\TinyMce\\TinyMceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
